{
  "name": "stiks",
  "version": "1.1.4",
  "description": "Node bundling & workflow utility for building Node apps.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "stx": "bin/cli"
  },
  "scripts": {
    "clean": "node ./build clean",
    "copy": "node ./build copy",
    "compile": "node ./build compile",
    "bump": "node ./build bump",
